<ul class="pager">
    <% if (page.prev){ %>
        <li class="previous"><a href="<%- config.root %><%- page.prev_link %>"><i class="fa fa-angle-left"></i>&nbsp;<%- __('prev')%></a></li>
    <% } %>
    <% if (page.next){ %>
        <li class="next"><a href="<%- config.root %><%- page.next_link %>"><%- __('next')%>&nbsp;<i class="fa fa-angle-right"></i></a></li>
    <% } %>
</ul>